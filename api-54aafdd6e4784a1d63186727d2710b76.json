      "schema": "type Query {\n  Prediction(name: String): PredictionResult\n  UseFirstName(first_name: String! = \"\", last_name:String! = \"\"): PredictionResult\n  UseLastName(fullname: FullName): PredictionResult\n  AgePrediction(name: String!): AgePredictionResult\n  GenderPrediction(name: String!): GenderPredictionResult\n  NationPrediction(name: String!): NationalityPredictionResult\n}\n\ninput FullName {\n  first_name: String! = \"\"\n  last_name: String! = \"\"\n}\n\ntype PredictionResult{\n  name: String\n  \"\"\"Predict the age of a person based on their name.\"\"\"\n  age: Int\n  \"\"\"Predict the gender of a person based on their name.\"\"\"\n  gender: String\n  \"\"\"Predict the nationality of a person based on their name.\"\"\"\n  country: String\n}\n\ntype AgePredictionResult {\n  count: Int\n  name: String\n  age: Int\n}\n\ntype GenderPredictionResult {\n  count: Int\n  name: String\n  gender: String\n  probability: Float\n}\n\ntype NationalityPredictionResult {\n  name: String\n  country: [NationProbability]\n}\n\ntype NationProbability {\n  country_id: String!\n  probability: Float!\n}",
